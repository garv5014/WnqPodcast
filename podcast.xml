---
layout: null
---

<?xml version='1.0' encoding='utf-8'?>
<rss version='2.0' xmlns:content='http://purl.org/rss/1.0/modules/content/' xmlns:itunes='http://www.itunes.com/dtds/podcast-1.0.dtd'>
    <channel>
        <title>{{ site.podcast.title }}</title>
        <description>{{ site.podcast.description }}</description>
        <link>{{ '/' | absolute_url }}</link>
        <author>{{ site.podcast.author }}</author>
        <email>{{ site.podcast.email }}</email>
        <image href='{{ site.podcast.logo | absolute_url }}'/>
        <language>{{ site.podcast.language }}</language>
        <copyright>copyright Â© {{ site.podcast.author }}</copyright>
        <owner>
            <name>{{ site.podcast.author }}</name>
            <email>{{ site.podcast.email }}</email>
        </owner>
        <category text='{{ site.podcast.category }}'>
            <category text='{{ site.podcast.subcategory }}'/>
        </category>
        <type>{{ site.podcast.type }}</type>
        <explicit>{{ site.podcast.explicit }}</explicit>
        <complete>{{ site.podcast.complete }}</complete>
        <block>{{ site.podcast.block }}</block>
        {% for post in site.categories.podcast %}
        <item>
            <title>{{ post.title }}</title>
            <author>{{ post.author }}</author>
            <season>{{ post.season }}</season>
            <episode>{{ post.episode }}</episode>
            <episodeType>{{ post.episodeType }}</episodeType>
            <explicit>{{ post.explicit }}</explicit>
            <link>{{ post.url | absolute_url }}</link>
            <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
            <duration>{{ post.length | xml_escape }}</duration>
            <enclosure url='{{ post.audio | absolute_url }}' length='{{ post.length | xml_escape }}'/>
            <description>{{ post.content | xml_escape | strip_newlines }}</description>
        </item>
        {% endfor %}
    </channel>
</rss>